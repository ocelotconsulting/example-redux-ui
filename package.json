{
  "name": "example-redux-ui",
  "version": "0.0.1",
  "description": "Skeleton redux UI",
  "scripts": {
    "clean": "rm -rf public && rm -rf dist && mkdir public",
    "dev": "supervisor -w server.js,index.html,dev.js,api node server",
    "build:copy-images": "cp -r images public/images",
    "build:copy-html": "cp index.html public/index.html",
    "build:less": "lessc --clean-css css/styles.less public/styles.css",
    "build:webpack": "webpack",
    "build": "npm run clean && npm-run-all build:*",
    "start": "NODE_ENV=production node server",
    "test": "mocha",
    "test:dev": "mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ocelotconsulting/example-redux-ui.git"
  },
  "keywords": [
    "react",
    "redux",
    "ui"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocelotconsulting/example-redux-ui/issues"
  },
  "homepage": "https://github.com/ocelotconsulting/example-redux-ui#readme",
  "engines": {
    "node": ">=6.7.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "express": "~4.14.0",
    "lodash": "~4.17.2",
    "moment": "~2.17.1"
  },
  "devDependencies": {
    "babel-core": "~6.22.1",
    "babel-loader": "~6.2.10",
    "babel-plugin-transform-class-properties": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-polyfill": "~6.22.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "~6.22.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-runtime": "~6.22.0",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "classnames": "~2.2.5",
    "enzyme": "~2.7.0",
    "less": "~2.7.2",
    "less-middleware": "~2.2.0",
    "less-plugin-clean-css": "~1.5.1",
    "mocha": "~3.2.0",
    "mockery": "~2.0.0",
    "npm-run-all": "~4.0.1",
    "react": "~15.4.0",
    "react-addons-test-utils": "~15.4.0",
    "react-dom": "~15.4.0",
    "react-redux": "~5.0.2",
    "react-router": "~3.0.2",
    "redux": "~3.6.0",
    "redux-logger": "~2.7.4",
    "redux-thunk": "~2.2.0",
    "sinon": "~1.17.7",
    "sinon-as-promised": "~4.0.2",
    "sinon-chai": "~2.8.0",
    "superagent": "~3.4.1",
    "supervisor": "~0.12.0",
    "webpack": "~2.2.1",
    "webpack-dev-middleware": "~1.9.0",
    "webpack-hot-middleware": "~2.16.1"
  }
}

{
  "name": "example-redux-ui",
  "version": "0.0.1",
  "description": "Skeleton redux UI",
  "scripts": {
    "clean": "rm -rf public && rm -rf dist && mkdir public",
    "dev": "supervisor -w server.js,index.html,dev.js,api node server",
    "build:copy-images": "cp -r images public/images",
    "build:copy-html": "cp index.html public/index.html",
    "build:webpack": "NODE_ENV=production webpack",
    "build": "npm run clean && npm-run-all build:*",
    "start": "NODE_ENV=production node server",
    "test": "mocha",
    "test:dev": "mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ocelotconsulting/example-redux-ui.git"
  },
  "keywords": [
    "react",
    "redux",
    "ui"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocelotconsulting/example-redux-ui/issues"
  },
  "homepage": "https://github.com/ocelotconsulting/example-redux-ui#readme",
  "engines": {
    "node": ">=6.7.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "express": "^4.16.1",
    "lodash": "^4.17.2",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "mocha": "^4.0.1",
    "mockery": "^2.1.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.2.0",
    "react-test-renderer": "^16.0.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.19.0",
    "superagent": "^3.6.3",
    "supervisor": "^0.12.0",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
